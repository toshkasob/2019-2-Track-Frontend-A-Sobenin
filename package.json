{
	"name": "2019-2-Track-Frontend-A-Sobenin",
	"homepage": "https://toshkasob.github.io/2019-2-Track-Frontend-A-Sobenin",
	"description": "Mail.Ru Group Frontend Track course, 2019",
	"version": "0.1.0",
	"author": "SAV",
	"dependencies": {
		"@emotion/core": "^10.0.22",
		"@emotion/styled": "^10.0.22",
		"cross-env": "^6.0.3",
		"node-sass": "^4.13.0",
		"prop-types": "^15.7.2",
		"react": "^16.11.0",
		"react-dom": "^16.11.0",
		"react-redux": "^7.1.1",
		"react-router-dom": "^5.1.2",
		"react-scripts": "3.4.0",
		"redux": "^4.0.4",
		"redux-devtools-extension": "^2.13.8",
		"redux-thunk": "^2.3.0"
	},
	"devDependencies": {
		"@storybook/addon-actions": "^5.3.13",
		"@storybook/addon-links": "^5.3.13",
		"@storybook/addons": "^5.3.13",
		"@storybook/preset-create-react-app": "^1.5.2",
		"@storybook/react": "^5.3.13",
		"@wemake-services/stylelint-config-scss": "^0.1.4",
		"babel-plugin-lodash": "^3.3.4",
		"babel-plugin-styled-components": "^1.10.7",
		"check-engine": "^1.8.0",
		"eslint": "^6.6.0",
		"eslint-config-airbnb": "^18.0.1",
		"eslint-config-prettier": "^6.5.0",
		"eslint-plugin-babel": "^5.3.0",
		"eslint-plugin-import": "^2.18.2",
		"eslint-plugin-jsx-a11y": "^6.2.3",
		"eslint-plugin-prettier": "^3.1.1",
		"eslint-plugin-react": "^7.16.0",
		"gh-pages": "^2.1.1",
		"husky": "^3.0.9",
		"jest-puppeteer": "^4.4.0",
		"jscpd": "^2.0.15",
		"prettier": "^1.18.2",
		"pretty-quick": "^2.0.0",
		"puppeteer": "^2.1.1",
		"react-test-renderer": "^16.12.0",
		"remark-cli": "^7.0.0",
		"remark-lint": "^6.0.5",
		"remark-preset-lint-markdown-style-guide": "^2.1.3",
		"rosie": "^2.0.1",
		"stylelint": "^11.1.1",
		"stylelint-a11y": "^1.2.2",
		"stylelint-config-css-modules": "^1.5.0",
		"stylelint-no-unsupported-browser-features": "^3.0.2",
		"stylelint-webpack-plugin": "^1.0.3"
	},
	"browserslist": [
		"> 5%",
		"last 2 versions",
		"not ie < 11",
		"not ie_mob < 11",
		"not dead"
	],
	"scripts": {
		"predeploy": "npm run build",
		"deploy": "gh-pages -d build",
		"start": "react-scripts start",
		"build": "react-scripts build",
		"eject": "react-scripts eject",
		"format": "prettier --write '**/*.{js,jsx,json,md}'",
		"format:changed": "pretty-quick",
		"format:staged": "pretty-quick --staged",
		"lint": "npm run lint:js && npm run lint:dup && npm run lint:css",
		"lint:check": "eslint --print-config .eslintrc.json | eslint-config-prettier-check",
		"lint:js:fix": "npm run lint:js -- --fix",
		"lint:js": "eslint src/**/*.js -c .eslintrc.json",
		"lint:css": "stylelint 'src/**/*.{scss,css}' && stylelint --rd 'src/**/*.{scss,css}'",
		"lint:dup": "jscpd -g --threshold=1 tests docker",
		"clean": "rm -rf build",
		"test": "npm run test:engine && npm run lint",
		"test:endtoend": "jest -c endtoend/jest.config.js",
		"test:engine": "check-engine",
		"test:sec": "npm audit || true",
		"test:unit": "react-scripts test",
		"ci": "npm run test && npm run build && npm run test:sec",
		"storybook": "start-storybook -p 9009 -s public",
		"build-storybook": "build-storybook -s public"
	},
	"husky": {
		"hooks": {
			"pre-commit": "npm run lint && npm run format:staged"
		}
	},
	"engines": {
		"npm": ">=6",
		"node": ">=8"
	}
}
